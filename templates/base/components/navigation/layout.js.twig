$('#{{ oUIBlock.GetId() }}-back').on('click', function () {
    window.location = '{{ oUIBlock.GetBackUrl()|raw }}';
});

$('#{{ oUIBlock.GetId() }}-first').on('click', function () {
    $('#ibo-form-navigation').attr('action', ' {{ oUIBlock.GetUrlFirst() | raw }} ');
    $('#ibo-form-navigation').submit();
});

